{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:local": "VITE_API_URL=http://localhost:8090 pnpm run dev",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
